<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.decode.minhasfinancas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wallet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wallet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddDespesaActivity" android:theme="@style/AppThemeDespesa"></activity>
        <activity android:name=".AddReceitaActivity" android:theme="@style/AppThemeReceita"/>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".LoginActivity" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>